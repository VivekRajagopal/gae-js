{
  "name": "@mondomob/gae-js-storage",
  "version": "1.2.0",
  "description": "Tools for Google Cloud Storage",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc --build --verbose tsconfig.prod.json",
    "clean": "rm -rf dist/ coverage/ tsconfig.tsbuildinfo tsconfig.prod.tsbuildinfo",
    "emulator:start": "firebase emulators:start --project storage-tests",
    "format": "prettier --write \"./src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:ci": "firebase emulators:exec --only storage --project storage-tests jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/mlev/gae-js",
  "peerDependencies": {
    "@mondomob/gae-js-core": "^1.0.0",
    "@google-cloud/storage": "^5.15.3",
    "express": ">= 4.17.1"
  },
  "devDependencies": {
    "@mondomob/gae-js-core": "1.3.0",
    "@google-cloud/storage": "5.18.0",
    "@types/express": "4.17.13",
    "express": "4.17.2"
  }
}
